  <li class="menu-item"><%= link_to current_user.email, edit_user_registration_path %></li>
  <li class="menu-item"><%= link_to 'Sign out', destroy_user_session_path, :method=>'delete' %></li>
<% else %>
  <li class="menu-item"><%= link_to 'Sign in', new_user_session_path %></li>
